{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build && node ./scripts/generate-cf-files.js",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "generate": "pnpm run generate:openapi",
    "generate:openapi": "openapi-typescript $(bash ./scripts/get-doc-url.sh) -o ./src/__generated__/nerve-centre-schema.d.ts",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.3.4",
    "@builder.io/qwik": "^1.4.5",
    "@builder.io/qwik-city": "^1.4.5",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.6",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.56.0",
    "eslint-plugin-qwik": "^1.4.5",
    "graphql": "^16.8.1",
    "openapi-typescript": "^6.7.4",
    "postcss": "^8.4.31",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.3.3",
    "undici": "5.22.0",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.0.0"
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.8.2",
    "@amcharts/amcharts5-fonts": "^5.0.1",
    "@amcharts/amcharts5-geodata": "^5.1.2",
    "@builder.io/qwik-labs": "github:BuilderIo/qwik-labs-build#main",
    "@lucia-auth/adapter-sqlite": "^3.0.0",
    "@qwikest/icons": "^0.0.13",
    "@upstash/redis": "^1.28.4",
    "arctic": "^1.2.0",
    "flowbite": "^2.3.0",
    "gql.tada": "^1.2.1",
    "lucia": "^3.0.1",
    "moment-timezone": "^0.5.45",
    "openapi-fetch": "^0.9.2",
    "oslo": "^1.1.2"
  }
}
